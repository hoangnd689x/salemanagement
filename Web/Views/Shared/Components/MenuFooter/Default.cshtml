@model List<Web.Models.ProductTypeViewModel>

@for (int i = 0; i < Model.Count; i++)
{
    var dem = 0;
    @for (int j = 0; j < Model.Count; j++)
    {
        if (Model[j].ParentId == Model[i].Id)
        {
            dem++;
        }
    }

    @if (Model[i].ParentId == 0 && dem > 0)
    {
        <li class="mnufter">
            <h2><a class="w" href="/Pages/kiem-tra-don-hang.aspx">@Model[i].Name</a></h2>
            @for (int j = 0; j < Model.Count; j++)
            {
                @if (Model[j].ParentId == Model[i].Id)
                {
                    <h2><a href="/Pages/info.aspx#muahang">@Model[j].Name</a></h2>
                }
            }
        </li>
    }




}